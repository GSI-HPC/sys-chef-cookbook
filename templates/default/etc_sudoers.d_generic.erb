<%= template_header %>

<%- @defaults.each do |options| -%>
Defaults<%= options['user'] ? ":#{options['user']}" : '' %> <%= options['option'] %>
<%- end -%>

<%- @users.each do |name, users| -%>
User_Alias <%= name %> = <%= users.join(', ') %>
<%- end -%>

<%- @hosts.each do |name, hosts| -%>
Host_Alias <%= name %> = <%= hosts.join(', ') %>
<%- end -%>

<%- @commands.each do |name, commands| -%>
Cmnd_Alias <%= name %> = <%= commands.join(', ') %>
<%- end -%>

<%- @rules.each do |rule| -%>
<%= rule %>
<%- end -%>
