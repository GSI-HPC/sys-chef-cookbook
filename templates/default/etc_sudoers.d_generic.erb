<%= template_header %>

<% @users.each_pair do |name,users| -%>
User_Alias <%= name %> = <%= users.join(', ') %>
<%- end %>

<% @hosts.each_pair do |name,hosts| -%>
Host_Alias <%= name %> = <%= hosts.join(', ') %>
<%- end %>

<% @commands.each_pair do |name,commands| -%>
Cmnd_Alias <%= name %> = <%= commands.join(', ') %>
<%- end %>

<% @rules.each do |rule| -%>
<%= rule %>
<%- end %>
