[global]
store-plaintext-passwords = <%= @store_plaintext_passwords?'yes':'no' %>
<%- unless @proxy.empty? -%>
http-proxy-host       = <%= @proxy[:host] %>
http-proxy-port       = <%= @proxy[:port] %>
http-proxy-exceptions = <%= @proxy[:exceptions] %>
<%- end -%>
