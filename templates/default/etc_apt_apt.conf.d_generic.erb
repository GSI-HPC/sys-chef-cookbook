# /etc/apt/apt.conf.d/<%= @name %>
#
# DO NOT CHANGE THIS FILE MANUALLY!
#
# This file is managed by the Chef `sys::apt` cookbook.

<% @config.each do |key,values| %>
<% if values.kind_of? Array %>
<%= key %> { 
 <% values.each do |value| %>
 "<%= value %>";
 <% end %>
};
<% else %>
<%= key %> "<%= values %>";
<% end %>
<% end %>
